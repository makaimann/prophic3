(set-logic HORN)
(declare-fun inv1 ((Array Int Int) (Array Int Int) Int Int) Bool)
(declare-fun inv2
             ((Array Int Int) (Array Int Int) (Array Int Int) Int Int)
             Bool)

(assert (forall ((a (Array Int Int)) (b (Array Int Int)) (N Int))
  (=> true (inv1 a b 0 N))))
(assert (forall ((a (Array Int Int))
         (b (Array Int Int))
         (i Int)
         (N Int)
         (i1 Int)
         (b1 (Array Int Int)))
  (let ((a!1 (= (store b i (select a (- N i1))) b1)))
    (=> (and (inv1 a b i N) (< i N) a!1 (= i1 (+ i 1))) (inv1 a b1 i1 N)))))
(assert (forall ((a (Array Int Int))
         (b (Array Int Int))
         (i Int)
         (N Int)
         (c (Array Int Int)))
  (=> (and (inv1 a b i N) (>= i N)) (inv2 a b c 0 N))))
(assert (forall ((a (Array Int Int))
         (b (Array Int Int))
         (c (Array Int Int))
         (i Int)
         (N Int)
         (i1 Int)
         (c1 (Array Int Int)))
  (let ((a!1 (= (store c i (select b (- N i1))) c1)))
    (=> (and (inv2 a b c i N) (< i N) a!1 (= i1 (+ i 1))) (inv2 a b c1 i1 N)))))
(assert (forall ((a (Array Int Int))
         (b (Array Int Int))
         (c (Array Int Int))
         (i Int)
         (N Int)
         (i1 Int))
  (let ((a!1 (and (inv2 a b c i N)
                  (>= i N)
                  (<= 0 i1)
                  (< i1 N)
                  (not (= (select a i1) (select c i1))))))
    (=> a!1 false))))
(check-sat)
